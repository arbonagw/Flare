
import * from "Core/Materials/Master.material"

material MI_APC : Master
{
	set_texture_alias diffuseMap	TX_APC_D.png
	set_texture_alias normalMap		TX_APC_N.png
	set_texture_alias specMap		TX_APC_S.png

	technique main
	{
		pass main
		{
			fragment_program_ref PS_Master
			{
				param_named specularStrength	float 0.5
				param_named normalStrength		float 1.0
			}
		}
	}
}
