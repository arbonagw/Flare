
import * from "Generic/Materials/M_Main.material"

material MI_Airlock : M_Main
{
	set_texture_alias DiffuseMap TX_Airlock_D.png
	set_texture_alias NormalMap TX_Airlock_N.png
	set_texture_alias SpecularMap TX_Airlock_S.png

	technique
	{
		pass Diffuse
		{
			fragment_program_ref mainDiffusePixelHlsl
			{
				param_named mSpecularPower float 2.5
				param_named mSpecularIntensity float 0.5
			}
		}
	}
}